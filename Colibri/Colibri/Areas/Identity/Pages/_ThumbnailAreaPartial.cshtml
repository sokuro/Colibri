
@model IEnumerable<ApplicationUserCategoryTypesSubscriber>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <div class="thumbnail col-sm-10 col-sm-offset-1">
            <div class="row">
                <div class="col-sm-10">
                    <label class="text-primary" style="font-size:17px;">@item.CategoryTypes.Name</label>
                </div>
                <div class="col-sm-2 text-right">
                    <h4>@item.Id</h4>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-10">
                    <p>@item.ApplicationUserId</p>
                </div>
            </div>
        </div>
    }
}